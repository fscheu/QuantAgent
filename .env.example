# ===========================================
# Database Configuration
# ===========================================
# PostgreSQL (recommended for production)
DATABASE_URL=postgresql://user:password@localhost:5432/quantagent

# SQLite (development/testing only)
# DATABASE_URL=sqlite:///./quantagent.db

# ===========================================
# LLM API Keys
# ===========================================
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
DASHSCOPE_API_KEY=your_qwen_key_here

# ===========================================
# LLM Provider Configuration
# ===========================================
# Supported providers: openai, anthropic, qwen
AGENT_LLM_PROVIDER=anthropic
GRAPH_LLM_PROVIDER=anthropic

# ===========================================
# LLM Model Configuration
# ===========================================
# Models are auto-selected based on provider if not specified
# OpenAI models: gpt-4o-mini, gpt-4o
# Anthropic models: claude-haiku-4-5-20251001, claude-sonnet-4-5-20250929
# Qwen models: qwen3-max, qwen3-vl-plus

AGENT_LLM_MODEL=claude-haiku-4-5-20251001
GRAPH_LLM_MODEL=claude-haiku-4-5-20251001

# ===========================================
# LLM Temperature
# ===========================================
# Professional, deterministic outputs (do not increase without justification)
AGENT_LLM_TEMPERATURE=0.1
GRAPH_LLM_TEMPERATURE=0.1
